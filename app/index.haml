!!! 5
%html{lang: 'en'}
  %head
    %meta{charset: 'UTF-8'}
    %meta{name: 'viewport', content:'width=device-width'}
    %link{rel: 'stylesheet',href:'css/gridlex.css' }
    %link{rel: 'stylesheet',href:'css/normalize.css' }
    %link{rel: 'stylesheet',href:'css/style.css' }
  %body
    %div#site.wrapper
      %header.header.grid-center
        %div.col-10.grid.navigation
          %div.col-2{'push-right':'off-2'}
            %img{src: 'images/logo-caicyt.png'}
          %div.col-8.grid-middle
            %div.col-12
              %ul.menu
                %li
                  %a{href:'#'} Inicio
                %li
                  %a{href:'#'} Explorar según
                %li
                  %a{href:'#'} Herramientas y documentos
                %li
                  %a{href:'#'} Acerca dess
        %div.col-12.grid-middle.grid-center.filters
          %div.col-10.grid-middle
            %div.col-3.label
              Buscar publicación
            %div.col-5.input{'push-right':'off-1'}
              %input{type:'text'}
            %div.col-3.advanced
              Búsqueda avanzada
      %section#body.wrapper
    %div#footer
      %script{src: 'js/init.js'}